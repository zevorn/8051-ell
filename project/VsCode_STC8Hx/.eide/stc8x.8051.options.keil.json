{
    "version": 2,
    "beforeBuildTasks": [],
    "afterBuildTasks": [
        {
            "name": "clean",
            "command": "cd \"${OutDir}\" && del *._* *.lst *.lnp"
        }
    ],
    "global": {
        "ram-mode": "LARGE",
        "rom-mode": "LARGE"
    },
    "c/cpp-compiler": {
        "optimization-type": "SPEED",
        "optimization-level": "level-8"
    },
    "asm-compiler": {},
    "linker": {
        "remove-unused": true,
        "output-format": "elf",
        "use-RTX": "None"
    }
}